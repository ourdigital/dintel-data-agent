# API 인증 정보 예제
# 실제 사용 시 이 파일을 'api_credentials.yaml'로 복사하고 필요한 자격 증명 정보를 입력하세요
# 주의: api_credentials.yaml 파일은 .gitignore에 포함되어 있어서 Git에 커밋되지 않습니다

# Google API Authentication (Enhanced)
google:
  client_id: "YOUR_CLIENT_ID"
  client_secret: "YOUR_CLIENT_SECRET"
  refresh_token: "YOUR_REFRESH_TOKEN"
  # Service account (recommended for production)
  service_account_file: "path/to/service-account-key.json"
  scopes:
    - "https://www.googleapis.com/auth/analytics.readonly"
    - "https://www.googleapis.com/auth/spreadsheets"
    - "https://www.googleapis.com/auth/webmasters.readonly"

# Google Analytics 4 (Enhanced)
google_analytics:
  property_id: "123456789"  # GA4 property ID
  
# Google Sheets (New)
google_sheets:
  spreadsheet_id: "your_spreadsheet_id"
  default_worksheet: "raw_data"

# Google Search Console
google_search_console:
  site_url: "https://example.com"

# Google Ads
google_ads:
  customer_id: "123-456-7890"
  developer_token: "YOUR_DEVELOPER_TOKEN"

# Meta (Facebook/Instagram) API
meta:
  app_id: "YOUR_APP_ID"
  app_secret: "YOUR_APP_SECRET"
  access_token: "YOUR_LONG_LIVED_ACCESS_TOKEN"
  ad_account_id: "act_123456789"

# YouTube API
youtube:
  channel_id: "YOUR_CHANNEL_ID"

# Naver SearchAd API
naver_searchad:
  api_key: "YOUR_API_KEY"
  secret_key: "YOUR_SECRET_KEY"
  customer_id: "YOUR_CUSTOMER_ID"

# Kakao API (필요한 경우)
kakao:
  rest_api_key: "YOUR_REST_API_KEY"
  admin_key: "YOUR_ADMIN_KEY"